{
    "name": "Rabus/Printar",
    "description": "Print your objects and arrays with ease with Printar, a print_r output formatter",
    "keywords": ["printar", "print_r", "php", "debugging", "pre", "composer", "package"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Rabus",
            "email": "rabus@art-by-rab.com"
        }
    ],
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "friendsofphp/php-cs-fixer": "*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Rabus\\Printar\\": "src/"
        }
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer --rules=@PSR1, @PSR2"
    }
}
